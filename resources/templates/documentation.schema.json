{
  "$id": "",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Process Documentation",
  "description": "A template for documenting a (business) process.",
  "required": [ "process" ],
  "type": "object",
  "properties": {
	"process": {
      "type": "string",
      "description": "The process name."
    },
	"processResponsibilityrocess": {
      "type": "string",
      "description": "The process responsibility."
    },
	"input": {
      "type": "string",
      "description": "The process input."
    },
	"supplierOfInput": {
      "type": "string",
      "description": "The supplier of the process input."
    },
	"processSteps": {
      "type": "number",
      "description": "The number of process steps."
    },
	"output": {
      "type": "string",
      "description": "The process output."
    },
	"customerOfOutput": {
      "type": "string",
      "description": "The customer of the process output."
    },
	"metrics": {
      "type": "string",
      "description": "The process metrics."
    },
	"it-systems": {
      "type": "string",
      "description": "The it-systems involved in the process."
    },
	"ressources": {
      "type": "string",
      "description": "The process ressources."
    }
	  
  }
  }
